{
  "name": "wax-matcher-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy": "aws s3 sync --acl public-read dist/ s3://waxmatcher.com",
    "deployWWW": "aws s3 sync --acl public-read dist/ s3://www.waxmatcher.com",
    "deployCodeBuild": "aws s3 sync --acl public-read dist/ s3://waxmatcher.com",
    "deployCodeBuildWWW": "aws s3 sync --acl public-read dist/ s3://www.waxmatcher.com",
    "invalidateCloudfront": "aws cloudfront create-invalidation --distribution-id $DISTRIBUTION_ID --paths /*"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.0",
    "@mui/types": "^7.2.4",
    "aws-amplify": "^5.2.1",
    "install": "^0.13.0",
    "js-cookie": "^3.0.5",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "npm": "^9.6.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  }
}
